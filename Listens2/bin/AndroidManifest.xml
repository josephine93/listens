<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.senseos.listens"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.senseos.listens.SStartMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.senseos.listens.SOver"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.OVER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
               <activity
            android:name="com.senseos.listens.SOpgenomenLuistermoment"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.SOPGENOMENLUISTERMOMENT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
               <activity
            android:name="com.senseos.listens.SPlanningLuistermoment"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.SPLANNINGLUISTERMOMENT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
